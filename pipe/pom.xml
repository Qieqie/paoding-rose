ld>    <distributionManagement>    <repository>        <id>releases</id>        <name>Releases</name>        <url>http://nexus.wjpush.com/nexus/content/repositories/releases/</url>    </repository>    <snapshotRepository>        <id>snapshots</id>        <name>Snapshots</name>        <url>http://nexus.wjpush.com/nexus/content/repositories/snapshots/</url>    </snapshotRepository>    </distributionManagement></project>